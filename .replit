modules = ["python-3.11", "cpp-clang20"]
[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Run C++"

[[workflows.workflow]]
name = "Run C++"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "clang++ -o main main.cpp && ./main"

[workflows.workflow.metadata]
outputType = "console"

[[ports]]
localPort = 5000
externalPort = 80

[nix]
packages = ["tree", "wget"]
